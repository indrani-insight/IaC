    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus"
        },
        "virtualNetworkName": {
            "value": "RIPA-Dev-VNet"
        },
        "resourceGroup": {
            "value": "RIPA-Dev-RG"
        },
        "addressSpaces": {
            "value": [
                "172.16.0.0/16"
            ]
        },
        "ipv6Enabled": {
