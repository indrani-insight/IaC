        "subnet0_name": {
            "value": "RIPA-Dev-Subnet1"
        },
        "subnet0_addressRange": {
            "value": "172.16.0.0/24"
        },
        "ddosProtectionPlanEnabled": {
            "value": false
        },
        "firewallEnabled": {
            "value": false
        },
        "bastionEnabled": {
            "value": false
        }
    }
